{
  "name": "maker-ramp-edge",
  "version": "2.0.9",
  "description": "Client library for fetching product groups from API",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.6.1",
    "maker-ramp-edge": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "rimraf": "^5.0.10",
    "jest": "^29.6.1",
    "babel-jest": "^29.6.1",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "products",
    "api",
    "react"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:js",
    "build:js": "babel src -d dist --source-maps",
    "prepare": "npm run build",
    "test": "jest",
    "print-products": "node ./bin/print-products.js"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testPathIgnorePatterns": ["/dist/"],
    "moduleFileExtensions": ["js", "json", "node"]
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
